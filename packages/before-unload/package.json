{
  "name": "@techmefr/before-unload",
  "version": "1.0.0",
  "description": "Composable for handling beforeunload and navigation guards in Vue applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "pnpm run build && pnpm run test:run"
  },
  "keywords": [
    "vue",
    "composable",
    "beforeunload",
    "navigation-guard",
    "typescript"
  ],
  "author": "techmefr",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.1",
    "happy-dom": "^20.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.1",
    "vue": "^3.5.0",
    "vue-router": "^4.3.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "vue-router": {
      "optional": true
    }
  }
}
